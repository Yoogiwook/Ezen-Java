import java.util.Scanner;

public class Day220421 {

	public static void main(String[] args) {
		// 세 정수를 입력받아서 최대값 출력(배열x)
		Scanner sc = new Scanner(System.in);
		
		System.out.println("정수 3개 입력");
		int num = sc.nextInt();
		int num2 = sc.nextInt();
		int num3 = sc.nextInt();
		int max = num;
		
		if(num2 > max) { 
			max = num2;
		}
		else if(num3 > max)
			max = num3;
		
		System.out.println("max = " + max);
		
		
	}

}
